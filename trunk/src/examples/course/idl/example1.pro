PRO example1 
        a=5                     ;sets a to 5 
        b=13.5                  ;and b to 13.5 
        c=a+b                   ;adds a and b and puts the result in c 
        d=a*sin(b)              ;sets d to the product of a and Sine(b) 
        e=b*exp(a)              ;sets e to the product of b and Exp(a) 
        print,a,b,c,d,e         ;prints the results 
END