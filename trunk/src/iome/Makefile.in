# List of subdirectories
SUBDIRS=utils simulation genericsimulationlib

all :
	@set -e; for i in $(SUBDIRS); do (cd $$i; make all -f Makefile ) || exit 1; done


install :
	@set -e; for i in $(SUBDIRS); do (cd $$i; make install -f Makefile ) || exit 1; done




clean :
	@set -e; for i in $(SUBDIRS); do (cd $$i; make clean -f Makefile ) || exit 1; done


