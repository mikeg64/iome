#make include file for building IoSimulation on felix

CC = g++
COPTFLAGS = -O 
CLINKER = ${CC}
FLAGS = -V 3.2.3
INCLUDES = -I$(XERCESCROOT)/include -I$(XALANCROOT)/include -I../IoUtils

LINKFLAGS = -g -Wall -Wno-deprecated
VPATH = src:../include

OPTFLAGS= $(COPTFLAGS)

LD_FLAGS =	-g -DCWDEBUG
LIBS	=	-lcwd -lm -L$(XERCESCROOT)/lib -L$(XALANCROOT)/lib -lxerces-c -lxalan-c
C_FLAGS	=	-g -DCWDEBUG -fpermissive

MYLIBS = ../lib/libIoUtils.a ../IoUtils/IoFileUtils.o


